{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
"outputs": [".next/**", "!.next/cache/**", ".vercel/output/**", "outputs","apps/frontend/.next/**", "apps/backend/dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
    "inputs": ["src/**", "src/index.ts"]
,
      "outputs": [

      ],
      "env": ["NODE_ENV"]
    }
  }
}
